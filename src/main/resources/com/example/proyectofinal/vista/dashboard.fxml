<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Pos?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.proyectofinal.controlador.DashboardController" spacing="30" alignment="CENTER" style="-fx-background-color: #f4f4f4; -fx-padding: 50px;">
    <Label text="Welcome to your Dashboard" style="-fx-font-size: 24px; -fx-font-weight: bold;" />
    <Button text="Log Out" onAction="#handleLogout" style="-fx-background-color: #f44336; -fx-text-fill: white; -fx-padding: 10px 20px; -fx-font-size: 16px; -fx-border-radius: 5px; -fx-pref-width: 200px;" />
</VBox>
